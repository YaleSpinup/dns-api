dns:
  log_level: info
  accounts:
    bam-dev:
      provider: proteus
      cidr_file: config/cidr_test.json
      config:
        url: <%= ENV['PROTEUS_URL'] %>
        username: <%= ENV['PROTEUS_USER'] %>
        password: <%= ENV['PROTEUS_PASS'] %>
        viewid: <%= ENV['PROTEUS_VIEWID'] %>
auth:
  token:
    <%= ENV['PROTEUS_API_X_AUTH_TOKEN'] %>:
      - POST
      - PUT
      - GET
      - DELETE
